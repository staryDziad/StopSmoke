<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OsiagnieciaPorazka">

    <TextView
        android:id="@+id/txIleDoSukcesu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Do sukcesu pozostalo X."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btWrocOsiagnieciaPorazka"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wróć"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txIleDoSukcesu"
        app:layout_constraintVertical_bias="0.74" />

    <ImageView
        android:id="@+id/imDrogaDoCelu2"
        android:layout_width="293dp"
        android:layout_height="283dp"
        app:layout_constraintBottom_toTopOf="@+id/txIleDoSukcesu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/droga" />
</androidx.constraintlayout.widget.ConstraintLayout>